@article{Behrendt2010Eventrelated,
  author = {Behrendt, J\"{o}rg and Gibbons, Henning and
                  Schrobsdorff, Hecke and Ihrke, Matthias and
                  Herrmann, J. Michael and Hasselhorn, Marcus},
  year = {2010},
  title = {Event-related brain potential correlates of identity
                  negative priming from overlapping pictures},
  journal = {Psychophysiology},
  publisher = {Blackwell Publishing Inc},
  volume = {47},
  number = {5},
  pages = {921--930},
  doi = {10.1111/j.1469-8986.2010.00989.x},
  url = {http://dx.doi.org/10.1111/j.1469-8986.2010.00989.x},
  abstract = {Abstract Event-related potentials (ERPs) were
                  obtained from an identity priming task, where a
                  green target had to be selected against a
                  superimposed red distractor. Several priming
                  conditions were realized in a mix of control (CO),
                  negative priming (NP), and positive priming (PP)
                  trials. PP and NP effects in reaction times (RTs)
                  were significant. ERP results conceptually replicate
                  earlier findings of left-posterior P300 reduction in
                  PP and NP trials compared to CO. This ERP effect may
                  reflect the detection of prime-probe similarity
                  corresponding to the concept of a retrieval cue. A
                  novel finding concerned amplitude increase of the
                  frontal late positive complex (LPC) in the order NP,
                  CO, and PP. NP therefore seemed to induce brain
                  activity related to cognitive control and/or memory
                  processes, with reduced LPC amplitude indicating
                  effortful processing. Overall, retrieval-based
                  explanations of identity NP are supported.},
  issn = {0048-5772},
  tags = {eigenes;np;psychology},
  pdf = {pdf/Behrendt_2010.pdf},
  date_added = {1318418781},
  localdate_added = {Wed Oct 12 13:26:21 2011},
}
